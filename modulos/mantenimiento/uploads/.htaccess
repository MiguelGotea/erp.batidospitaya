# Permitir solo imágenes
<FilesMatch "\.(jpg|jpeg|png|gif|webp)$">
    Order allow,deny
    Allow from all
</FilesMatch>

# Denegar todo lo demás
<FilesMatch "^(?!.*\.(jpg|jpeg|png|gif|webp)$).*$">
    Order allow,deny
    Deny from all
</FilesMatch>

# Prevenir ejecución de PHP
<Files *.php>
    Order allow,deny
    Deny from all
</Files>
